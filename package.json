{
  "name": "vutinajs",
  "version": "1.0.2",
  "description": "一个TS开发的FP函数工具库",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "@types/index.d.ts",
  "unpkg": "dist/index.umd.min.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "eslint": "eslint --fix package --ext .ts --max-warnings=0",
    "prettier": "prettier --write \"package/**/*.ts\"",
    "commit": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "build": "npm run eslint && npm run prettier && rollup -c --bundleConfigAsCjs",
    "dev": "ts-node-dev --respawn --transpile-only ./package/index.ts"
  },
  "keywords": [
    "ts",
    "js",
    "函数库",
    "函数式",
    "工具"
  ],
  "author": "辛克莱",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "conventional-changelog-cli": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "rollup": "^4.6.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run eslint",
      "npm run prettier"
    ]
  }
}
